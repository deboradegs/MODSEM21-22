[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Se una bestia ha pericolosità maggiore o uguale a 3, allora è una bestia magica pericolosa"^^xsd:string ;
   rdfs:label "pericolosità >= 3 -> Bestia pericolosa"^^xsd:string ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#Bestia> ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#b>
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#propertyPredicate> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#pericolosità> ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#b> ;
                                                                  <http://www.w3.org/2003/11/swrl#argument2> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#n>
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#BuiltinAtom> ;
