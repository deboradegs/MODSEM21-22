<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#b> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#p> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#d> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#n> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#m> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#q1> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#q2> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#q3> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#f> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#g> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Se una bestia ha come proprietario un personaggio hallora esegue la volontà anche dell'antenato di quel personaggio"^^xsd:string ;
   rdfs:label "haProprietario ^ ancestorOf -> esegue volontà di antenato"^^xsd:string ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#haProprietario> ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#b> ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#p>
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#classPredicate> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#Bestia> ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#b>
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#classPredicate> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#Personaggio> ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#p>
                                                                           ] ;
                                                                 rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                            rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                                        <http://www.w3.org/2003/11/swrl#propertyPredicate> <http://purl.org/vocab/relationship/ancestorOf> ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#p> ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument2> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#d>
                                                                                      ] ;
                                                                            rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                                       rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                                                                   <http://www.w3.org/2003/11/swrl#classPredicate> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#Personaggio> ;
                                                                                                   <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#d>
                                                                                                 ] ;
                                                                                       rdf:rest rdf:nil
                                                                                     ]
                                                                          ]
                                                               ]
                                                    ]
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#haProprietario> ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> <http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#b> ;
