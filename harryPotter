    <!-- http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#esegueVolontàDi -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#esegueVolontàDi">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#Personaggio"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/39345/ontologies/2022/3/harryPotter#Mago"/>
        <rdfs:comment xml:lang="ita">cosa che esegue la volontà di questo personaggio, può essere un altro personaggio oppure un incantesimo, una bacchetta, ecc...</rdfs:comment>
        <rdfs:label xml:lang="ita">esegue volontà di</rdfs:label>
