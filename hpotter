        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment>In Harry Potter le bestie riconoscono come padroni i successori del loro primo padrone</rdfs:comment>
        <rdfs:label>discendente di proprietario -&gt;proprietario</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
