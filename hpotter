    <!-- http://www.semanticweb.org/39345/ontologies/2022/2/hpotter#Casata -->

    <owl:Class rdf:about="http://www.semanticweb.org/39345/ontologies/2022/2/hpotter#Casata">
        <owl:equivalentClass>
